@model WebApresentation.Models.ClienteModel

@{
	ViewBag.Title = "CadView";
}

<h2>Cliente</h2>
@using (Html.BeginForm("CadView", "Cliente", FormMethod.Post, new { id = "fmCliente", enctype = "multipart/form-data"}))

<div class="row">
	<div class="col-md-10">
		<div class="form-group">
			<label class="control-label">
				@Html.LabelFor(x => x.nomeCliente)
			</label>
			<div class="controls">
				@Html.TextBoxFor(m => m.nomeCliente, new { @class = "span12 m-warp", maxlength = "100" })
				<span class="coloRed help-block">
					@Html.ValidationMessageFor(m => m.nomeCliente)
				</span>
			</div>
		</div>
	</div>
</div>

